<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite">

	
	<groups>
		<run>
			<include name="Purchase"></include>
		</run>
	</groups>
	
	<listeners>
		<listener
			class-name="EcommerceApplicationPractice.TestComponents.Listeners"></listener>
	</listeners>

	<test thread-count="5" name="Submit Orde Test">
		<classes>
			<class name="EcommerceApplicationPractice.Tests.SubmitOrderTest" />
		</classes>
	</test>

	<test thread-count="5" name="Error Validation Test">
		<classes>
			<class name="EcommerceApplicationPractice.Tests.ErrorValidation" />
		</classes>
	</test>


</suite>